{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "type": "webroot",
        "version": "5.2.1",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/5.2.1.zip"
        },
        "require": {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    }
  ],
  "require": {
    "wordpress/wordpress": "5.2.1",
    "fancyguy/webroot-installer": "1.1.0",
    "wpackagist-plugin/autodescription": "3.*",
    "wpackagist-plugin/autoptimize": "2.*",
    "wpackagist-plugin/classic-editor": "1.*",
    "wpackagist-plugin/google-analytics-for-wordpress": "7.*",
    "wpackagist-plugin/imsanity": "2.*",
    "wpackagist-plugin/query-monitor": "3.*",
    "wpackagist-plugin/stream": "3.*",
    "wpackagist-plugin/wordfence": "7.*",
    "wpackagist-plugin/wp-mail-smtp": "1.*",
    "wpackagist-plugin/wp-optimize": "2.*"
  },
  "extra": {
    "installer-paths": {
      "app/themes/{$name}": [
        "type:wordpress-theme"
      ],
      "app/plugins/{$name}": [
        "type:wordpress-plugin"
      ]
    },
    "webroot-dir": "wp",
    "webroot-package": "wordpress/wordpress"
  }
}